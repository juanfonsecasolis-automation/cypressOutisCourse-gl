{
  "name": "cypressoutiscourse-gl",
  "version": "1.0.0",
  "description": "Automated software testing with Cypress",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:runquiet": "cypress run --quiet",
    "cy:run:chrome": "cypress run --browser chrome --headless",
    "cy:run:firefox": "cypress run --browser firefox --headless",
    "cy:run:electron": "cypress run --browser electron --headless",
    "cy:run-dashboard": "cypress run --record --key 441cb098-1bcc-4b0b-b6a7-578c1eb1869a"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juanfonsecasolis-gl/cypressOutisCourse-gl.git"
  },
  "keywords": [
    "testing",
    "cypress",
    "automation",
    "api",
    "BDD"
  ],
  "author": "Juan Manuel Fonseca Solis",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/juanfonsecasolis-gl/cypressOutisCourse-gl/issues"
  },
  "homepage": "https://github.com/juanfonsecasolis-gl/cypressOutisCourse-gl#readme",
  "dependencies": {
    "cypress": "^7.2.0",
    "cypress-cucumber-preprocessor": "^2.5.5",
    "prettier": "^2.2.1"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  }
}
